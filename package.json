{
  "name": "llm-x",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env DEBUG=vite-plugin-pwa SW_DEV=true vite --force",
    "build": "tsc && SW=true vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg",
    "prepare": "husky install",
    "test": "vite build && vitest",
    "test:ui": "vite build && vitest --ui",
    "test:watch": "concurrently 'vitest' 'vite build --watch'"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,md,html,json}": [
      "npx prettier --write"
    ],
    "*.{ts,tsx,js,jsx,html}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@langchain/community": "^0.0.45",
    "axios": "^1.6.8",
    "camelcase-keys": "^9.1.3",
    "framer-motion": "^11.0.25",
    "gh-pages": "^6.1.1",
    "highlight.js": "^11.9.0",
    "kbar": "^0.1.0-beta.45",
    "lodash": "^4.17.21",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7",
    "mobx-state-tree": "^5.4.1",
    "moment": "^2.30.1",
    "mst-persist": "^0.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.51.2",
    "react-markdown": "^9.0.1",
    "react-scrollable-feed": "^2.0.1",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "use-media": "^1.5.0",
    "vite-tsconfig-paths": "^4.3.2",
    "yet-another-react-lightbox": "^3.17.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.43.0",
    "@rollup/plugin-replace": "^5.0.5",
    "@tailwindcss/typography": "^0.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.5",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.4.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "daisyui": "^4.10.1",
    "eslint": "^9.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "fishery": "^2.2.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.13",
    "playwright": "^1.43.0",
